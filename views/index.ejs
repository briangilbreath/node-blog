<% layout('layout') -%>

	<div class="jumbotron text-center">
		<h1>Brian Gilbreath</h1>


	</div>

	
	<div class="text-center">

	<% if(todos != '') { %>
			<% todos.forEach( function ( todo ){ %>
			  <p>
			  
			  	 <span>
			      <a href="/view/<%= todo.slug %>" title="View this todo item"><%= todo.title %></a>
			    </span>

			  </p>
			<% }); %>

			  	    
			    <a href="/page/<%- pageNum + 1%>"> Page <%- pageNum + 1 %> </a>


	<% }else{ %>

	 No content.

	<% } %>
			    
	</div>
